 <!-- Music Section -->
 
 
<div id="music-section" class="Albums py-5">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="titlepage mb-4">
                    <h3 style="font-size: 25px; font-weight:bold;">Your Favorite Tunes</h3>
                    <span>Discover handpicked tracks that will elevate your mood. From timeless classics to the latest hits, let the rhythm take over.</span>
                </div>
            </div>
        </div>
        <div class="row">
            <% music.forEach(music => { %>
            <div class="col-lg-3 col-md-4 col-sm-6 mb-4">
                <div class="card h-100">
                    <img src="<%= music.imageUrl %>" class="card-img-top" alt="<%= music.title %>">
                    <div class="card-body">
                        <h5 class="card-title mb-1" style="text-align: left;"><%= music.title %></h5>
                        <p class="card-text" style="text-align: left;">By <strong><%= music.author %></strong></p>
                    </div>
                    <div class="card-footer d-flex justify-content-between">
                        
                        <audio controls>
  <source src="<%= music.fileUrl %>" type="audio/ogg">
  
</audio>
                        
                    </div>
                </div>
            </div>
            <% }); %>
        </div>
    </div>
</div>
<!-- Video Section -->
<div id="video-section" class="Albums py-5">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="titlepage mb-4">
                    <h3 style="font-size: 25px; font-weight:bold;" class="titlepage" >Our Curated Videos</h3>
                    <span>Take a break and explore our collection of captivating visuals. From music videos to short films, enjoy every frame.
                    </span>
                </div>
            </div>
        </div>
        <div class="row">
            <% videos.forEach(video => { %>
            <div class="col-lg-3 col-md-4 col-sm-6 mb-4">
                <div class="card h-100">
                    <img src="<%= video.imageUrl %>" class="card-img-top" alt="<%= video.title %>">
                    <div class="card-body">
                        <h5 class="card-title mb-1" style="text-align: left;"><%= video.title %></h5>
                        <p class="card-text" style="text-align: left;">By <strong><%= video.author %></strong></p>
                    </div>
                    <div class="card-footer d-flex justify-content-between">
                        <a href="<%= video.fileUrl %>" class="btn btn-danger">
                            <i class="fas fa-play"></i> Watch
                        </a>
                        <a href="<%= video.fileUrl %>" download class="btn btn-secondary">
                            <i class="fas fa-download"></i> Download
                        </a>
                    </div>
                </div>
            </div>
            <% }); %>
        </div>
    </div>
